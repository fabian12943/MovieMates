#authentication-login.modal.c-modal.authentication-modal{ "aria-hidden": true, "aria-labelledby": "authentication-login", tabindex: -1 }
  .modal-dialog.modal-dialog-centered
    .modal-content
      .modal-header
        %a.float-start{"data-bs-target": "#authentication-overview", "data-bs-toggle": "modal", type: "button"}
          %span.fa-solid.fa-chevron-left.back-modal
        %h1= t '.sign_in'
        %a.float-end{"aria-label" => "Close", "data-bs-dismiss" => "modal", :type => "button"}
          %span.close-icon.fa-solid.fa-xmark.close-modal
      .modal-body
        = render partial: "shared/authentication/sign_in_form"
        %hr
        = render partial: "shared/authentication/omniauth_links"
        